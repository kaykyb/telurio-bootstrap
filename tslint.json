{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": true,
    "trailing-comma": false,
    "arrow-parens": false,
    "ordered-imports": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
